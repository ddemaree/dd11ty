{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "devCommand": "next dev",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Accept-CH",
          "value": "Sec-CH-Prefers-Color-Scheme"
        }
      ]
    }
  ]
}
