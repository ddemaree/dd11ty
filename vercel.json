{
  "framework": "astro",
  "rewrites": [
    {
      "source": "/wf",
      "destination": "/api/proxy"
    },
    {
      "source": "/wf/:path*",
      "destination": "/api/proxy"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Accept-CH",
          "value": "Sec-CH-Prefers-Color-Scheme"
        }
      ]
    }
  ]
}
